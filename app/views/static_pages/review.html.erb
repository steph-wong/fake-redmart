<% provide(:title, "Review") %>

<!DOCTYPE html>
<html>
  <head>
    <title><%= yield(:title) %> | </title>
  </head>
  <body>
    <h1>Review</h1>

    <div class="row">
      <aside class="col-md-4">
        <section class="user_info">
          <%= render 'shared/user_info' %>
        </section>
        <section class="review_form">
          <%= render 'shared/review_form', review: Review.new %>
        </section>
      </aside>
    </div>

  </body>
</html>
